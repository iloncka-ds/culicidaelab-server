# Local development environment configuration for CulicidaeLab
# Domain and SSL configuration (no SSL for local)
DOMAIN=localhost
EMAIL=dev@localhost
SSL_ENABLED=false
STAGING=false

# Registry and version (use local images or latest)
REGISTRY=iloncka/
FRONTEND_VERSION=0.1.1
SSL_NGINX_VERSION=0.1.4
BACKEND_VERSION=0.1.1
NGINX_VERSION=0.1.2

# CORS and URLs (localhost configuration)
CORS_ORIGINS=http://localhost,http://localhost:8765,http://127.0.0.1,http://127.0.0.1:8765,http://localhost:3000,http://127.0.0.1:3000
CLIENT_BACKEND_URL=http://nginx
SERVER_BACKEND_URL=http://backend:8000
STATIC_FILES_URL=http://localhost
STATIC_URL_BASE=http://localhost

# Environment
ENVIRONMENT=development
NGINX_ENV=development
FASTAPI_ENV=development
DEBUG=true
SOLARA_DEBUG=true

# Backend configuration
CULICIDAELAB_BACKEND_CORS_ORIGINS=http://localhost,http://localhost:8765,http://127.0.0.1,http://127.0.0.1:8765,http://localhost:3000,http://127.0.0.1:3000
CULICIDAELAB_SAVE_PREDICTED_IMAGES=true